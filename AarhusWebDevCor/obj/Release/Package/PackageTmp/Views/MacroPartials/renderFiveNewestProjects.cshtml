@inherits Umbraco.Web.Macros.PartialViewMacroPage


@{ var selection = Umbraco.TypedContent(1065).Children.OrderBy("CreateDate desc").Take(3); }

@* OrderBy() takes the property to sort by and optionally order desc/asc *@

<ul class="list-group">
    @foreach (var item in selection)
    {
        <li class="list-group-item"><a href="@item.Url">@item.Name</a>
			</li>
        
    }
</ul>
	